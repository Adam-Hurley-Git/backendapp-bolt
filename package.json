{
  "name": "calendar-extension-backend-sveltekit",
  "version": "1.0.0",
  "description": "Backend for Chrome extension with Supabase and Paddle integration - SvelteKit",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "svelte-kit sync && vite build",
    "preview": "vite preview",
    "preview:worker": "npm run build && wrangler pages dev .svelte-kit/cloudflare",
    "deploy": "npm run build && wrangler pages deploy",
    "check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
    "lint": "eslint .",
    "wrangler:types": "wrangler types --env-interface CloudflareEnv"
  },
  "keywords": [
    "chrome-extension",
    "supabase",
    "paddle",
    "payments",
    "sveltekit",
    "cloudflare"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@supabase/ssr": "^0.5.0",
    "@supabase/supabase-js": "^2.38.5",
    "clsx": "^2.0.0",
    "date-fns": "^2.30.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241127.0",
    "@sveltejs/adapter-cloudflare": "^5.0.0",
    "@sveltejs/kit": "^2.15.0",
    "@sveltejs/vite-plugin-svelte": "^5.0.0",
    "@tailwindcss/forms": "^0.5.7",
    "@types/node": "^20.10.4",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.56.0",
    "eslint-plugin-svelte": "^2.46.0",
    "postcss": "^8.4.32",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "tailwindcss": "^3.3.6",
    "typescript": "^5.3.3",
    "vite": "^6.0.0",
    "wrangler": "^4.43.0"
  }
}
